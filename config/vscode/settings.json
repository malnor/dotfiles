{
    "editor.tabSize": 2,
    "editor.detectIndentation": true,
    "editor.renderControlCharacters": true,
    "editor.renderWhitespace": "all",
    "window.zoomLevel": -1,
    "workbench.statusBar.visible": true,
    "workbench.colorTheme": "Material Palenight for Operator Mono",
    "files.exclude": {
        "**/.git": true,
        "**/.DS_Store": true,
        "**/*.js.map": true,
        "**/node_modules": true,
        "**/*.js": {
            "when": "$(basename).ts"
        }
    },
    "search.exclude": {
        "**/.git": true,
        "**/.DS_Store": true,
        "**/*.js.map": true,
        "**/node_modules": true,
        "**/*.js": {
            "when": "$(basename).ts"
        }
    },
    "files.trimTrailingWhitespace": true
}